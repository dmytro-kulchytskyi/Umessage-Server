{
  "appPort": 80,
  "redis": {
    "host": "127.0.0.1",
    "port": 6379
  },
  "data-provider": {
    "providers": {
      "vk": {
        "name": "vk",
        "apiVersion": "5.69",
        "apiLink": "https://api.vk.com/",
        "apiMethods": {
          "getDialogs": {
            "path": "method/messages.getDialogs"
          },
          "getMessages": {
            "path": "method/messages.getHistory"
          },
          "getUserInfo": {
            "path": "method/users.get"
          },
          "getLPServer": {
            "path": "method/messages.getLongPollServer",
            "needPts": "1"
          }
        },
        "lpServerConfiguration": {
          "lpVersion": 2,
          "mode": 98,
          "wait": 15
        }
      }
    }
  }
}